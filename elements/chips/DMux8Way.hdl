/**
 * 8-way demultiplexor:
 * {a, b, c, d, e, f, g, h} = {in, 0, 0, 0, 0, 0, 0, 0} if sel == 000
 *                            {0, in, 0, 0, 0, 0, 0, 0} if sel == 001
 *                            etc.
 *                            {0, 0, 0, 0, 0, 0, 0, in} if sel == 111
 */

CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    DMux(in=in, sel=sel[2], a=aout, b=bout);

    DMux(in=aout, sel=sel[1], a=aaout, b=bbout);
    DMux(in=bout, sel=sel[1], a=cout, b=dout);

    DMux(in=aaout, sel=sel[0], a=a, b=b);
    DMux(in=bbout, sel=sel[0], a=c, b=d);
    DMux(in=cout, sel=sel[0], a=e, b=f);
    DMux(in=dout, sel=sel[0], a=g, b=h);
}