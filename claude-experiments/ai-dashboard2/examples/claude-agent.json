{
  "id": "claude-agent",
  "title": "Claude Agent",
  "subtitle": "Powered by Claude Agent SDK",
  "icon": "console",
  "theme": {
    "bgApp": "#1a1a2e",
    "textHead": "system-ui, sans-serif",
    "textBody": "system-ui, sans-serif",
    "accent": "#e94560",
    "textColor": "#eaeaea",
    "positive": "#00d9ff",
    "negative": "#ff6b9d",
    "widgetBg": "rgba(22, 27, 34, 0.9)",
    "widgetBorder": "1px solid rgba(233, 69, 96, 0.3)",
