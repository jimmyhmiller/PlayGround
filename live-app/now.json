{
    "rewrites": [
        {"source": "/", "destination": "/"},
        {"source": "/demo", "destination": "/demo"},
        {"source": "/api", "destination": "/api"},
        {"source": "/api/(.*)", "destination": "/api?route=$1"},
        {"source": "/admin/entity", "destination": "/api/entity"},
        {"source": "/admin/collection", "destination": "/api/collection"}
    ],
    
    "env": {
        "DATABASE_SECRET": "@live-app-server-db-prod"
      }
}