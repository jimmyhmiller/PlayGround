          <xs:documentation>
            Amount of credit limit increase requested.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="memo" type="xs:string" minOccurs="0"   >
        <xs:annotation>
          <xs:documentation>
            A general memo field.  This field can be used to provide details on why an increase was requested. 
          </xs:documentation>
        </xs:annotation>
      </xs:element>

    </xs:sequence>
  </xs:complexType>
  
</xs:schema>